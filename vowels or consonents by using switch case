import java.util.*;
public class Main {
	public static void main(String[] args) {
		
		System.out.println("enter a alphabet ");
		Scanner s=new Scanner(System.in);
		String str=s.next();
		char ch=str.charAt(0);
		
		switch(ch)
		{
			case 'a':
			case 'e':
			case 'i':
			case 'o':
			case 'u':
		case 'A':
		case 'E':
		case 'I':
		case 'O':
		case 'U':System.out.println("given alphabet id vowel");
		break;
		default:System.out.println("given alphabet id consonant");
		break;
		}
	}
}

out put:
enter a alphabet
a
given alphabet is vowel
