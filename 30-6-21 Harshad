import java.util.*;
out put:

class Harshad
{
	public static void main(String args[]) 
	{
	int n,r,s=0;
	Scanner Sc=new Scanner(System.in);
	n=Sc.nextInt();
	int k=n;
	while(n>0)
	{
	    r=n%10;
	    n=n/10;
	    s=s+r;
	}
	if(k%s==0)
	{
	System.out.println("Harshd number");
	}
	else
	{
	    System.out.println("not a Harshad number");
	}
	}
}

out put:
156
Harshd number
